def dec_to_binary(n):
	temp = 0 #чтобы было понятнее другим (и самому себе) при прочтении вашего кода, называйте временные переменные как "t" или "temp" (лучше temp, так понятнее), от слова temporary (временный)
	tempstr = '' #аналогично, но под "str" понимается string (строка), так вы даёте себе и другим знать, что это именно переменная типа string, а не что-то ещё 
	while (n > 0):
		temp = n % 2 #остаток от деления
		n = n // 2 #только кратное кол-во раз
		#print(temp, end=" ") #с помощью end задаём своё окончание вывода, чтобы он не делал перенос на новую строку по умолчанию, а чтобы писал например либо пробел, либо запятую с пробелом, либо ничего (""), поэкспериментируйте
		tempstr += str(temp)
	return tempstr

num = (int(input("Введите целое число в x10: ")))
binary = dec_to_binary(num) #string
print("Ваше двоичное число: ", binary[::-1])